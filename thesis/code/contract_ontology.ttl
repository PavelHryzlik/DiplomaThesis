  rdfs:range xsd:string ;
  rdfs:subPropertyOf dcterms:type ;
  rdfs:isDefinedBy <http://tiny.cc/open-contracting> .
  
:subjectType a owl:DatatypeProperty, owl:FunctionalProperty ;
